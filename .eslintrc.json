{
  "extends": "airbnb",
  "globals": {
    "document": true,
    "describe": true,
    "beforeEach": true,
    "context": true,
    "expect": true,
    "fetch": true,
    "Headers": true,
    "it": true,
    "React": true,
    "Request": true,
    "window": true
  },
  "rules": {
    "react/sort-prop-types": 2,
    "react/jsx-sort-props": 2
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true
  }
}