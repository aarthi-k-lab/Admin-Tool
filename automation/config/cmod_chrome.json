{
  "test_workers": {
    "enabled": true,
    "workers": 5
  },
  "launch_url": "http://localhost",
  "selenium_port": 4444,
  "selenium_host": "localhost",
  "silent": true,
  "screenshots": {
    "enabled": false,
    "path": "./reports"
  },
  "desiredCapabilities": {
    "unexpectedAlertBehaviour": "accept",
    "browserName": "chrome",
    "chromeOptions": {
      "args": [
        "disable-web-security",
        "--headless",
       "ignore-certificate-errors",
       "disable-infobars",
       "--test-type"
      ]
    }
  },
    "end_session_on_fail": true,
    "detailed_output": false
  }
